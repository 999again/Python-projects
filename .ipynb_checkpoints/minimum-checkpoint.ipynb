   "source": [
    "numbers = input(\"Enter a list of numbers:\")\n",
    "new = numbers.split()\n",
    "nums = [int(x) for x in new]\n",
    "min_number = nums[0]\n",
    "for num in nums:\n",
    "    if num < min_number:\n",
    "        min_number = num\n",
    "print(min_number)"
