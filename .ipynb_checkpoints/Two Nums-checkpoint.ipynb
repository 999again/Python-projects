    "def fun(num, target):\n",
    "    for i in range(len(num)):\n",
    "        for j in range(i + 1, len(num)):\n",
    "            if num[i] + num[j] == target:\n",
    "                return [i, j]\n",
    "        \n",
    "num = [2, 7, 11, 15]\n",
    "target = 9\n",
    "\n",
    "print(fun(num, target))"
   ]
